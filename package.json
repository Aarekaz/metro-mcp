{
  "name": "metro-mcp",
  "version": "1.0.0",
  "description": "MCP server for US transit systems (DC Metro, NYC Subway) on Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
